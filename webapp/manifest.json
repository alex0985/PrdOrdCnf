{
	"_version": "1.9.0",
	"sap.app": {
		"id": "alwe.ProdOrderConf",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZPROD_ORDER_CONFIRMATION_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.2masterdetail",
			"version": "1.52.7"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "alwe.ProdOrderConf.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.42.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ushell": {},
				"sap.collaboration": {
					"lazy": true
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "alwe.ProdOrderConf.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultCountMode": "None",
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay"
				}
			},
			"search": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/searchHelp.json",
				"preload": true
			},
			"selection": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/selection.json",
				"preload": true
			},
			"operData": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "alwe.ProdOrderConf.view",
				"controlId": "idAppControl",
				"controlAggregation": "detailPages",
				"bypassed": {
					"target": [
						"master",
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "master",
					"target": [
						"object",
						"master"
					]
				},
				{
					"pattern": "Order/{orderId}",
					"name": "oper",
					"target": [
						"operation"
					]
				},
				{
					"pattern": "OperationSet/{objectId}",
					"name": "object",
					"target": [
						"master",
						"object"
					]
				}
			],
			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "master",
					"controlAggregation": "masterPages",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "alwe.ProdOrderConf.view",
					"controlId": "idAppControl",
					"bypassed": {
						"target": [
							"master",
							"notFound"
						]
					},
					"async": true
				},
				"operation": {
					"viewName": "Operations",
					"viewLevel": 1,
					"viewId": "operations",
					"controlAggregation": "masterPages",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "alwe.ProdOrderConf.view",
					"async": true,
					"clearControlAggregation": false
				},
				"object": {
					"viewName": "Detail",
					"viewId": "detail",
					"viewLevel": 2,
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "alwe.ProdOrderConf.view",
					"controlId": "idAppControl",
					"controlAggregation": "detailPages",
					"bypassed": {
						"target": [
							"master",
							"notFound"
						]
					},
					"async": true
				},
				"detailObjectNotFound": {
					"viewName": "DetailObjectNotFound",
					"viewId": "detailObjectNotFound",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "alwe.ProdOrderConf.view",
					"controlId": "idAppControl",
					"controlAggregation": "detailPages",
					"bypassed": {
						"target": [
							"master",
							"notFound"
						]
					},
					"async": true
				},
				"detailNoObjectsAvailable": {
					"viewName": "DetailNoObjectsAvailable",
					"viewId": "detailNoObjectsAvailable",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "alwe.ProdOrderConf.view",
					"controlId": "idAppControl",
					"controlAggregation": "detailPages",
					"bypassed": {
						"target": [
							"master",
							"notFound"
						]
					},
					"async": true
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound",
					"routerClass": "sap.m.routing.Router",
					"viewType": "XML",
					"viewPath": "alwe.ProdOrderConf.view",
					"controlId": "idAppControl",
					"controlAggregation": "detailPages",
					"bypassed": {
						"target": [
							"master",
							"notFound"
						]
					},
					"async": true
				},
				"Operations": {
					"viewType": "XML",
					"viewName": "Operations"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zprdordcnf/webapp",
		"_version": "1.1.0"
	}
}